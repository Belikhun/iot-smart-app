{
	"appId": "dev.belikhun.smartapp",
	"appName": "Smart App",
	"webDir": "dist",
	"android": {
		"package": "dev.belikhun.smartapp",
		"allowMixedContent": true
	},
	"server": {
		"androidScheme": "http",
		"allowNavigation": ["*"],
		"cleartext": true
	},
	"plugins": {
		"Keyboard": {
			"resize": "body",
			"resizeOnFullScreen": true
		},

		"PushNotifications": {
			"presentationOptions": ["badge", "sound", "alert"]
		}
	}
}
